  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#969896",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.other"],
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#81a2be"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Title",
      "scope": ["entity.name.section", "markup.heading"],
      "settings": {
        "foreground": "#8abeb7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#2b79a2"
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#c5c8c6"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "YAML Key",
      "scope": ["entity.name.tag.yaml"],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Escape Character",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#8abeb7"
      }
    },
    {
      "name": "Built-in",
      "scope": ["support.function.builtin", "entity.name.function.macro"],
      "settings": {
        "foreground": "#de935f"
      }
    },
    {
      "name": "Type Parameter",
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Lifetime",
      "scope": ["storage.modifier.lifetime.rust", "entity.name.lifetime.rust"],
      "settings": {
        "foreground": "#de935f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Macro",
      "scope": ["entity.name.function.macro.rust", "meta.macro.rust"],
      "settings": {
        "foreground": "#8abeb7"
      }
    }
  ]
}
